{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "jing-kou-ling",
  "title": "京口令",
  "description": "generate and parse JD code ｜ 用于生成和解析京东口令",
  "icon": "jdCodeIcon.png",
  "author": "zooPanda",
  "license": "MIT",
  "commands": [
    {
      "name": "enjcode",
      "title": "生成口令",
      "subtitle": "生成京口令",
      "description": "generate JD code | 生成京东口令",
      "mode": "view"
    },
    {
      "name": "dejcode",
      "title": "解析口令",
      "subtitle": "解析京东口令",
      "description": "parse JD code | 解析京东口令",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.28.0",
    "axios": "^0.25.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/node": "~16.10.0",
    "@types/react": "^17.0.28",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "react-devtools": "^4.19.2",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint"
  }
}